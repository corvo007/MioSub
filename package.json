{
  "name": "miosub",
  "description": "Smart, Playful, and Powerful AI Subtitle Generator",
  "author": "Corvo007",
  "license": "AGPL-3.0-only",
  "private": true,
  "version": "3.0.8",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "preinstall": "git config --global url.\"https://github.com/\".insteadOf ssh://git@github.com/",
    "postinstall": "node scripts/download-binaries.mjs",
    "download:binaries": "node scripts/download-binaries.mjs",
    "download:binaries:force": "node scripts/download-binaries.mjs --force",
    "dev": "vite",
    "build": "vite build",
    "docs:dev": "cd docs && npx fumadocs-mdx && next dev -p 3001",
    "docs:build": "cd docs && npx fumadocs-mdx && next build",
    "i18n:extract": "node scripts/extract-chinese.mjs",
    "i18n:check": "node scripts/check-i18n.mjs",
    "i18n:check:missing": "node scripts/check-i18n.mjs --check-missing",
    "i18n:check:unused": "node scripts/check-i18n.mjs --check-unused",
    "docs:analytics": "npx tsx scripts/extract-analytics.ts",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron -r ts-node/register electron/main.ts\"",
    "electron:build": "yarn build:main && vite build && electron-builder",
    "build:debug": "cross-env DEBUG_BUILD=true yarn electron:build",
    "build:main": "vite build -c vite.config.electron.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\" \"electron/**/*.ts\"",
    "lint": "eslint src electron --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@amplitude/analytics-node": "^1.5.32",
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/genai": "^1.30.0",
    "@ricky0123/vad-web": "^0.0.30",
    "@sentry/electron": "^7.6.0",
    "@uiw/react-json-view": "^2.0.0-alpha.40",
    "assjs": "^0.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "eld": "^2.0.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.7.3",
    "fluent-ffmpeg": "^2.1.3",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonrepair": "^3.13.2",
    "lucide-react": "^0.562.0",
    "mixpanel": "^0.20.0",
    "onnxruntime-web": "^1.23.2",
    "openai": "^6.9.1",
    "p-map": "^7.0.4",
    "pathe": "^2.0.3",
    "react": "^19.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.0",
    "react-rnd": "^10.5.2",
    "react-virtuoso": "^4.17.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@sentry/vite-plugin": "^4.7.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/mdx": "^2.0.13",
    "@types/mixpanel": "^2.14.9",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react": "^5.0.0",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.22",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "framer-motion": "^12.27.0",
    "fumadocs-core": "^16.4.7",
    "fumadocs-mdx": "^14.2.5",
    "fumadocs-ui": "^16.4.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mermaid": "^11.12.2",
    "next": "^16.1.3",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-static-copy": "^3.1.4",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.miosub.app",
    "productName": "MioSub",
    "directories": {
      "output": "release"
    },
    "icon": "resources/icon.png",
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "!node_modules/onnxruntime-web/**/*",
      "!node_modules/@ricky0123/vad-web/**/*",
      "resources/icon.png"
    ],
    "asarUnpack": [
      "dist/*.onnx",
      "dist/*.wasm",
      "dist/*.mjs",
      "dist/*.js"
    ],
    "extraResources": [],
    "mac": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "artifactName": "MioSub-mac-${arch}.${ext}",
      "extraResources": [
        {
          "from": "resources",
          "to": ".",
          "filter": [
            "whisper-cli",
            "yt-dlp",
            "qjs",
            "ffmpeg",
            "ffprobe",
            "cpp-ort-aligner",
            "libonnxruntime.dylib",
            "ggml-silero-v6.2.0.bin",
            "Chinese_to_Pinyin.txt",
            "fonts/**/*"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "MioSub-win-x64.${ext}",
      "extraResources": [
        {
          "from": "resources",
          "to": ".",
          "filter": [
            "whisper-cli.exe",
            "yt-dlp.exe",
            "qjs.exe",
            "ffmpeg.exe",
            "ffprobe.exe",
            "cpp-ort-aligner.exe",
            "*.dll",
            "ggml-silero-v6.2.0.bin",
            "Chinese_to_Pinyin.txt",
            "fonts/**/*"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "MioSub",
      "artifactName": "MioSub-win-x64-setup.${ext}"
    },
    "extraFiles": [],
    "afterPack": "scripts/afterPack.cjs",
    "afterAllArtifactBuild": "scripts/afterAllArtifactBuild.cjs",
    "linux": {
      "target": "AppImage",
      "artifactName": "MioSub-linux-${arch}.${ext}",
      "extraResources": [
        {
          "from": "resources",
          "to": ".",
          "filter": [
            "whisper-cli",
            "yt-dlp",
            "qjs",
            "ffmpeg",
            "ffprobe",
            "cpp-ort-aligner",
            "ggml-silero-v6.2.0.bin",
            "Chinese_to_Pinyin.txt",
            "fonts/**/*"
          ]
        }
      ]
    },
    "publish": {
      "provider": "github",
      "owner": "Corvo007",
      "repo": "Gemini-Subtitle-Pro"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  }
}
